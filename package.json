{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate project used for starting new projects!",
  "main": "index.js",
  "engines": {
    "node": "14.2.0"
  },
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "knex": "^0.21.1",
    "morgan": "^1.10.0",
    "uuid": "^8.1.0",
    "winston": "^3.2.1",
    "xss": "^1.0.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "pg": "^8.2.1",
    "postgrator-cli": "^3.2.0",
    "supertest": "^4.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JZPoteat/express-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/JZPoteat/express-boilerplate/issues"
  },
  "homepage": "https://github.com/JZPoteat/express-boilerplate#readme"
}
